pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/salamatali726200-cpu/<aapka-repo>.git'
            }
        }
        stage('Build') {
            steps {
                echo 'Building the project...'
            }
        }
        stage('Deploy') {
            steps {
                sh '''
                mkdir -p /var/www/html
                cp index.html /var/www/html/
                '''
            }
        }
    }
}

